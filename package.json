{
  "name": "tree-sitter-eta",
  "version": "0.0.1",
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test && tree-sitter parse examples/* --quiet --time"
  },
  "dependencies": {
    "nan": "2.17.0"
  },
  "devDependencies": {
    "node-gyp": "9.3.1",
    "tree-sitter-cli": "0.20.8",
    "typescript": "5.0.4"
  },
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "text.eta",
      "file-types": [
        "eta"
      ],
      "injection-regex": "eta"
    }
  ]
}
